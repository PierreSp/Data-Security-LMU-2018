{
  "manifest_version": 2,

  "name": "Data Security Plugin",
  "description": "This extension will change the User Agent",
  "version": "0.0.2",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": ["webRequest", "webRequestBlocking", "http://*/*", "https://*/*"],

  "background": {
  "scripts": ["background.js"]
}
}
