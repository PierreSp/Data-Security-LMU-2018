{
  "manifest_version": 2,

  "name": "Data Security Plugin",
  "description": "This extension will change the browser fingerprint based on tor browser",
  "version": "0.0.3",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": ["webRequest", "webRequestBlocking", "http://*/*", "https://*/*","*://*.localhost/*","https://www.google.com/"],

  "background": {
  "scripts": ["background.js"]
}
}
